<div class="container">
  <h1>Bookmark new</h1>
  <div class="container">
    <%= simple_form_for [@list, @bookmark] do |f| %>
      <%= f.input :comment %>
      <%# <%= f.select :movie_id, Movie.all.map { |m| m.title } %>
      <%= f.association :movie, collection: @movies, include_blank: false %>
      <%= f.submit %>
    <% end %>
    <%= link_to 'Back', list_path(@list) %>
  </div>
</div>
